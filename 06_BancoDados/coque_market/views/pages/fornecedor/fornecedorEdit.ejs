<section class="container">
    <section class="row">
        <article class="col">
            <h1 class="mt-5 text-center">Cadastrar Fornecedores</h1>
            <form action="/fornecedor/editar" method="POST">
                <article class="form-group">
                    <label for="nome">Nome</label>
                    <input class="form-control" type="text" id="nome" name="nome" placeholder="Nome do fornecedor" value="<%= fornecedor_lista.forn_nome %>">
                </article>
                <article class="form-group">
                    <label for="cnpj">CNPJ</label>
                    <input class="form-control" type="text" id="cnpj" name="cnpj" placeholder="0000" value="<%= fornecedor_lista.forn_cnpj %>">
                </article>
                <article>
                    <input type="hidden" name="id" id="id" value="<%= fornecedor_lista.forn_id %>">
                </article>
                <button id="submit" type="submit" class="mb-5 btn btn-primary">Enviar</button>
            </form>
        </article>
    </section>
</section>